<apex:page extensions="AttachPdfController" standardController="Student__c" renderAs="pdf">
    <apex:pageBlock title="Student information">
        <apex:pageBlockSection title="Personal information" columns="1">
            <apex:outputField value="{!Student__c.Name}"/>
            <apex:outputField value="{!Student__c.FirstName__c}"/>
            <apex:outputField value="{!Student__c.Age__c}"/>
            <apex:outputField value="{!Student__c.Sex__c}"/>
            <apex:outputField value="{!Student__c.Married__c}"/>
        </apex:pageBlockSection>
        <apex:pageBlockSection title="Contact information">
            <apex:outputField value="{!Student__c.Email__c}"/>
        </apex:pageBlockSection>
    </apex:pageBlock>
</apex:page>